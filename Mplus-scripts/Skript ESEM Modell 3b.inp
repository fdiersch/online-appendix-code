TITLE:   FREE – Modell 3a: ESEM mit Targetrotation (drei Faktoren, freie Kreuzladungen).
DATA: FILE = "free_subs_all_mplus.dat";
VARIABLE: 
  ! Variablennamen in der Reihenfolge der Spalten in der Datendatei
NAMES = CASE sex age version study
    AB01 AB02 AB03 AB04 AB05 AB06 AB07 AB08 AB09 AB10 AB11 AB12 AB13 
    Rel01 Rel02 Rel03 Rel04 Rel05 Rel06 Rel07 Rel08 Rel09 Rel10 Rel11 Rel12 Rel13 
    Pos01 Pos02 Pos03 Pos04 Pos05 Pos06 Pos07 Pos08 Pos09 Pos10 Pos11 Pos12 Pos13;

USEVARIABLES=
 AB01-AB13
    Rel01-Rel13
    Pos01-Pos13;

MISSING=.;  ! Systemwert für fehlende Werte

ANALYSIS: 
  ESTIMATOR = MLR;     ! robuster Maximum-Likelihood-Schätzer
  ! Keine Rotation spezifiziert
  ! mit vollständig freien Kreuzladungen zwischen allen drei Faktoren.

MODEL:
  ! Modell mit freien Kreuzladungen:
  ! Jedes Item lädt auf alle drei Faktoren (Absolutismus, Relativismus,
  ! Postrelativismus). Es werden keine Ladungen auf 0 fixiert.
  !
  ! Hinweis zu (*1):
  ! Mplus fixiert mit (*1) die erste Ladung in der jeweiligen Ladungsliste auf 1,
  ! um den Faktor zu skalieren. Alle weiteren Ladungen werden frei geschätzt.

   ! Faktor "Absolutismus" mit freien Ladungen auf alle FREE-Items
  ABSOLUT BY
    AB01-AB13        ! primäre Ladungen auf Absolutismus
    Rel01-Rel13~0    ! Ziel: nahe 0 für Relativismus-Items (Target)
    Pos01-Pos13~0 (*1);  ! Ziel: nahe 0 für Postrelativismus-Items (Target, erste Ladung fixiert)

  ! Faktor "Relativismus" mit freien Ladungen auf alle FREE-Items
  RELATIV BY
    Rel01-Rel13
    AB01-AB13~0
    Pos01-Pos13~0 (*1);

   ! Faktor "Postrelativismus" mit freien Ladungen auf alle FREE-Items
  POSTREL BY
    Pos01-Pos13
    AB01-AB13~0
    Rel01-Rel13~0 (*1);

OUTPUT:

STDYX;   ! standardisierte Parameter (STDYX)
TECH4;   ! Varianz-Kovarianz-Matrizen der latenten Variablen